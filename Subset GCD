# cook your dish here
import math
t=int(input())
for i in range(t):
    n,m=map(int,input().split())
    k=n//m
    ans=k
    if n//2<m:
        for i in range(1,m+1):
            print(i,end=' ')
    elif m==1:
        print(n)
    else:
        for i in range(0,m):
            print(ans,end=" ")
            ans+=k
    print()
