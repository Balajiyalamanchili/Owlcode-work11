class Solution:
    def findMin(self, nums: List[int]) -> int:
        return min(nums)
---------------OR-OR-OR-------------------------
class Solution:
    def findMin(self, n: List[int]) -> int:
        for i in range(len(n)-1):
            if n[i]>n[i+1]:
                return n[i+1]
        return n[0]
